 function muestraListaDeCompras() {
    var produce = ["manzanas", "naranjas"];
    var frozen = ["brócoli", "helado"];
    frozen.push("croqueta de papa");
    console.log(frozen);
}
// la función imrpime que frozen es igual a brócoli, helado, manzanas, naranjas, croqueta de papa 

/* DIAGRAMA EN T

variable = produce , frozen
valeus = manzanas, naranjas, brócoli, helado, croqueta de papa

*/



